  "globs": ["**/*.md"],
  "ignores": [
    ".git/**/*.md",
    ".fvm/**/*.md",
    ".idea/**/*.md",
    "**/.dart_tool/**/*.md",
    "**/ios/.symlinks/**/*.md",
    "**/ios/Pods/**/*.md",
    "**/ios/Runner/**/*.md"
  ]
}
